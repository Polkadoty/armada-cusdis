{
  "name": "cusdis",
  "scripts": {
    "dev": "DB_TYPE=sqlite npm run dev:general",
    "dev:pg": "DB_TYPE=pgsql npm run dev:general",
    "dev:mysql": "DB_TYPE=mysql npm run dev:general",
    "dev:general": "npm run db:deploy && npm run db:generate && next dev",
    "build": "DB_TYPE=pgsql npm run db:generate && DB_TYPE=pgsql npm run db:deploy && next build && npm run build:widget",
    "build:without-migrate": "npm run db:generate && next build && npm run build:widget",
    "db:deploy": "prisma migrate deploy --schema ./prisma/$DB_TYPE/schema.prisma",
    "db:generate": "prisma generate --schema ./prisma/$DB_TYPE/schema.prisma",
    "db:push": "DB_TYPE=sqlite npm run db:push:general",
    "db:push:pg": "DB_TYPE=pgsql npm run db:push:general",
    "db:push:mysql": "DB_TYPE=mysql npm run db:push:general",
    "db:push:general": "prisma db push --schema ./prisma/$DB_TYPE/schema.prisma --preview-feature",
    "db:migrate": "prisma migrate dev --schema ./prisma/$DB_TYPE/schema.prisma",
    "db:migrateAll": "prisma migrate dev --schema ./prisma/sqlite/schema.prisma && prisma migrate dev --schema ./prisma/pgsql/schema.prisma && prisma migrate dev --schema ./prisma/mysql/schema.prisma",
    "admin": "prisma studio",
    "widget": "vite --config scripts/vite.config.js",
    "start": "next start -p 3000",
    "start:with-migrate": "npm run db:generate && npm run db:deploy && next start -p 3000",
    "cp-widget-lang": "mkdir -p public/js/widget/lang && cp -r widget/lang public/js/widget",
    "build:widget": "npm run cp-widget-lang && vite build --config scripts/vite.widget.config.js && npm run build:sdk && npm run build:iframe && npm run build:count",
    "build:sdk": "vite build --config scripts/vite.sdk.config.js",
    "build:iframe": "vite build --config scripts/vite.iframe.config.js",
    "build:count": "vite build --config scripts/vite.count.config.js"
  },
  "prettier": "@egoist/prettier-config",
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/server": "^11.11.0",
    "@hapi/boom": "^10.0.1",
    "@mantine/core": "^7.4.1",
    "@mantine/form": "^7.4.1",
    "@mantine/hooks": "^7.4.1",
    "@mantine/modals": "^7.4.1",
    "@mantine/notifications": "^7.4.1",
    "@prisma/client": "^5.8.1",
    "@sendgrid/mail": "^8.1.0",
    "@sentry/node": "^7.92.0",
    "@sentry/tracing": "^7.92.0",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.5",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "formidable": "^3.5.1",
    "framer-motion": "^10.18.0",
    "jsonwebtoken": "^9.0.2",
    "markdown-it": "^14.0.0",
    "milligram": "^1.4.1",
    "mini-capture": "^0.1.1",
    "nanoid": "^5.0.4",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "next-connect": "^1.0.0",
    "nodemailer": "^6.9.8",
    "postcss": "^8.4.33",
    "postcss-preset-mantine": "^1.12.3",
    "postcss-simple-vars": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-icons": "^5.0.1",
    "react-query": "^3.39.3",
    "redaxios": "^0.5.1",
    "reflect-metadata": "^0.2.1",
    "rollup-plugin-svelte": "^7.1.6",
    "svelte": "^4.2.8",
    "svelte-preprocess": "^5.1.3",
    "tailwindcss": "^3.4.1",
    "turndown": "^7.1.2",
    "vite": "^5.0.11",
    "fast-xml-parser": "^4.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-decorators": "^7.23.7",
    "@egoist/prettier-config": "^1.0.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "prettier": "^2.8.8",
    "prisma": "^5.8.1",
    "typegraphql-prisma": "^0.27.1",
    "typescript": "^5.3.3"
  }
}
